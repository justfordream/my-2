<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
	<!-- 客户端Hessian代理工厂Bean -->
	<bean id="NotifyHandle" class="org.springframework.remoting.caucho.HessianProxyFactoryBean">
		<!-- 请求代理Servlet路径 -->
		<property name="serviceUrl">
			<value>http://127.0.0.1:8080/CMUPayCrmFront/remote/NotifyHandle</value>
		</property>
		<!-- 接口定义 -->
		<property name="serviceInterface">
			<value>com.huateng.core.adaper.rmi.NotifyHandle</value>
		</property>
	</bean>
</beans> 
